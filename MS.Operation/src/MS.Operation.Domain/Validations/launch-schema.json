{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "origin",
    "destination",
    "value"
  ],
  "properties": {
    "origin": {
      "$id": "#/properties/origin",
      "type": "object",
      "title": "The Origin Schema",
      "required": [
        "number"
      ],
      "properties": {
        "number": {
          "$id": "#/properties/origin/properties/number",
          "type": "integer",
          "title": "The Number Schema",
          "default": 0,
          "examples": [
            1141
          ],
          "minimum": 1.0
        }
      }
    },
    "destination": {
      "$id": "#/properties/destination",
      "type": "object",
      "title": "The Destination Schema",
      "required": [
        "number"
      ],
      "properties": {
        "number": {
          "$id": "#/properties/destination/properties/number",
          "type": "integer",
          "title": "The Number Schema",
          "default": 0,
          "examples": [
            1010
          ],
          "minimum": 1.0
        }
      }
    },
    "value": {
      "$id": "#/properties/value",
      "type": "number",
      "title": "The Value Schema",
      "default": 0.0,
      "examples": [
        1.5
      ],
      "minimum": 0.01
    }
  }
}